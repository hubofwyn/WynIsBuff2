{
    "name": "wynisbuff2",
    "description": "WynIsBuff2 - An extreme buff adventure game with triple jump mechanics and maximum gaming power!",
    "version": "2.0.0",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/verlyn13/WynIsBuff2.git"
    },
    "author": "WynIsBuff2 Team",
    "license": "MIT",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "bugs": {
        "url": "https://github.com/verlyn13/WynIsBuff2/issues"
    },
    "homepage": "https://github.com/verlyn13/WynIsBuff2#readme",
    "scripts": {
        "dev": "bun log.js dev & bunx --bun vite --config vite/config.dev.mjs",
        "build": "bun log.js build & bunx --bun vite build --config vite/config.prod.mjs",
        "dev-nolog": "bunx --bun vite --config vite/config.dev.mjs",
        "build-nolog": "bunx --bun vite build --config vite/config.prod.mjs",
        "test": "bun test",
        "lint": "bunx --bun eslint . --ext .js,.mjs",
        "lint:fix": "bunx --bun eslint . --ext .js,.mjs --fix",
        "format": "bunx --bun prettier --write \"**/*.{js,mjs,json,md}\"",
        "format:check": "bunx --bun prettier --check \"**/*.{js,mjs,json,md}\"",
        "generate-assets": "bun scripts/generate-assets.js",
        "validate-assets": "bun scripts/validate-assets.js",
        "prepare": "bunx --bun husky"
    },
    "devDependencies": {
        "@eslint/js": "^9.14.0",
        "eslint": "^9.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "husky": "^9.1.6",
        "lint-staged": "^15.2.10",
        "prettier": "^3.3.3",
        "terser": "^5.44.0",
        "vite": "^7.1.12"
    },
  "dependencies": {
        "@dimforge/rapier2d-compat": "^0.19.2",
        "howler": "^2.2.4",
        "phaser": "^3.90.0"
    },
    "alias": {
        "@features": "./src/features",
        "@core": "./src/core",
        "@observability": "./src/observability",
        "@scenes": "./src/scenes"
    }
}
